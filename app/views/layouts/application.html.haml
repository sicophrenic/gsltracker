!!! 5

%html
	%head
		%title GSL Tracker
		= stylesheet_link_tag 'application'
		= javascript_include_tag 'application'
		= csrf_meta_tags
	
	%body
		.container
			.header
				= render :partial => 'shared/header'
			= render :partial => 'shared/flash', :locals => { :flash => flash }
			.content
				= yield
			.footer
				= render :partial => 'shared/footer'